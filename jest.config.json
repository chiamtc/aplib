{
  "coverageDirectory": "./src/app/test",
  "transform": {
    "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
  },
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "modulePathIgnorePatterns": [
    "build",
    "node_modules"
  ],
  "setupFiles": [
    "<rootDir>/src/app/test/setup.js"
  ],
  "testPathIgnorePatterns": [
    "./cypress/integration"
  ],
  "testEnvironment": "jsdom"
}
